    "import datetime\n",
    "SUNDAY_WEEKDAY = 6\n",
    "DAYS_IN_A_WEEK = 7\n",
    "\n",
    "today = datetime.date.today() +  datetime.timedelta(0)\n",
    "start_date = today + datetime.timedelta( (SUNDAY_WEEKDAY-today.weekday()-1) % DAYS_IN_A_WEEK+1 )\n",
    "today.strftime(\"%b %d, %Y\"), start_date.strftime(\"%b %d, %Y\")"
   ]
  }
